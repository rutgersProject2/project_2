                                                <input class="form-check-input" name="Mycoplasm" value="negative" type="radio" checked="checked">
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-sm-offset-6">
                                        <div class="diagnosis_type" id="Mycoplasm">
                                            <span>Mycoplasm</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <div class="form-check-pos" id="lgv-pos">
                                            <label class="form-check-label">
                                                <input class="form-check-input" name="lgv" type="radio" value="positive">
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-check-neg" id="lgv-neg">
                                            <label class="form-check-label">
                                                <input class="form-check-input" name="lgv" value="negative" type="radio" checked="checked">
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-sm-3 col-sm-offset-6">
                                        <div class="diagnosis_type" id="lgv">
                                            <span>lgv</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </form>
                </p>
            </div>
            <div class="modal-footer">
                <button id="save_patient_info" type="button" class="btn btn-dark">Save Info</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<!-- Searching Patient Record Modal -->
<div class="modal" id="patient_searchModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Search for an existing record</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>
                    <form>
                        <div class="form-row">
                            <div class="form-group">
                                <label for="search_patient_firstName">First Name:</label>
                                <input class="form-control" id="search_patient_firstName" type="text">
                            </div>
                            <div class="form-group">
                                <label for="search_patient_lstName">Last Name:</label>
                                <input class="form-control" id="search_patient_lstName" type="text">
                            </div>
                            <div class="form-group">
                                <label for="search_patient_dob">Date of Birth:</label>
                                <input class="form-control" id="search_patient_dob" type="text">
                            </div>
                            <div class="form-group">
                                <label for="search_patient_ID">Patient ID:</label>
                                <input class="form-control" id="search_patient_ID" type="text">
                            </div>
                        </div>
                    </form>
                </p>
            </div>
            <div class="modal-footer">
                <button id="search_patient_info" type="button" class="btn btn-dark">Search</button>
            </div>
        </div>
    </div>
</div>

